<h1>People#new</h1>

# first_name, last_name, age, hair_color, eye_color, gender, alive

<h1>New People</h1>
<%= form_for @people do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :age %>
  <%= f.text_area :age %>

  <%= f.label :hair_color %>
  <%= f.text_area :hair_color %>

  <%= f.label :eye_color %>
  <%= f.text_area :eye_color %>

  <%= f.label :gender %>
  <%= f.text_area :gender %>

  <%= f.label :alive %>
  <%= f.text_area :alive %>

  <%= f.submit %>
<% end %>